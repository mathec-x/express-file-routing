{
  "name": "express-file-routing",
  "version": "1.2.0",
  "description": "Simple system-based file routing for Express",
  "author": "Matthias Halfmann",
  "repository": "matthiaaas/express-file-routing",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
    "example:js": "NODE_ENV=development node ./demo/js/index",
    "example:ts": "ts-node-dev --respawn --watch ./demo/ts/app.ts --ignore-watch node_modules --no-notify ./demo/ts/app.ts"
  },
  "keywords": [
    "express",
    "api",
    "file",
    "router",
    "routing",
    "typescript"
  ],
  "license": "MIT",
  "peerDependencies": {
    "express": ">4.1.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "express": "^4.17.1",
    "ts-node-dev": "^1.1.8",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "rollup": "^2.52.8",
    "rollup-plugin-typescript2": "^0.29.0",
    "typescript": "^4.3.5"
  },
  "files": [
    "dist"
  ]
}
