{
  "name": "express-file-routing",
  "version": "1.2.0",
  "description": "Simple system-based file routing for Express",
  "author": "Matthias Halfmann",
  "repository": "matthiaaas/express-file-routing",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:esm && npm run build:cjs",
    "demo:js": "NODE_ENV=development node ./demo/js/index",
    "demo:ts": "ts-node-dev --respawn --watch ./demo/ts/app.ts --ignore-watch node_modules --no-notify ./demo/ts/app.ts",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs"
  },
  "keywords": [
    "express",
    "api",
    "file",
    "router",
    "routing",
    "typescript"
  ],
  "license": "MIT",
  "peerDependencies": {
    "express": ">4.1.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "express": "^4.17.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "files": [
    "dist"
  ]
}
